<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
            http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">
    <bean id="solforgeGameStateFactory" class="com.gempukku.tcg.generic.SimpleGameStateFactory">
        <property name="gameStateBuilderMap">
            <util:map>
                <entry key="standard" value-ref="solforgeGameStateBuilder"/>
            </util:map>
        </property>
    </bean>

    <bean id="solforgeGameStateBuilder" class="com.gempukku.tcg.generic.SpringGameStateBuilder">
        <property name="gameObjects">
            <util:map>
                <entry value="solforgeGameEventEngine">
                    <key>
                        <util:constant static-field="com.gempukku.tcg.solforge.SolforgeObjects.GAME_EVENT_ENGINE"/>
                    </key>
                </entry>
                <entry value="solforgeModifierEngine">
                    <key>
                        <util:constant static-field="com.gempukku.tcg.solforge.SolforgeObjects.GAME_MODIFIER_ENGINE"/>
                    </key>
                </entry>
            </util:map>
        </property>
        <property name="playerObjects">
            <util:map>

            </util:map>
        </property>
    </bean>

    <bean id="solforgeGameEventEngine" class="com.gempukku.tcg.generic.event.GameEventEngine" scope="prototype">

    </bean>

    <bean id="solforgeGameModifierEngine" class="com.gempukku.tcg.generic.modifier.GameModifierEngine"
          scope="prototype">

    </bean>
</beans>