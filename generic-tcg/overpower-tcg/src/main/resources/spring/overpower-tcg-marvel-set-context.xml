<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
            http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

    <bean abstract="true" id="characterCard" class="com.gempukku.tcg.overpower.card.OverpowerCardBlueprint">
        <property name="cardType" value="character"/>
    </bean>
    
    <bean id="card.1-1" parent="characterCard">
        <property name="id" value="1-1"/>
        <property name="name" value="Apocalypse"/>
        <property name="energy" value="6"/>
        <property name="fighting" value="4"/>
        <property name="strength" value="4"/>
    </bean>

    <bean id="card.1-2" parent="characterCard">
        <property name="id" value="1-2"/>
        <property name="name" value="Beast"/>
        <property name="energy" value="1"/>
        <property name="fighting" value="5"/>
        <property name="strength" value="6"/>
    </bean>

    <bean id="card.1-3" parent="characterCard">
        <property name="id" value="1-3"/>
        <property name="name" value="Bishop"/>
        <property name="energy" value="5"/>
        <property name="fighting" value="6"/>
        <property name="strength" value="4"/>
    </bean>

    <bean id="card.1-4" parent="characterCard">
        <property name="id" value="1-4"/>
        <property name="name" value="Cable"/>
        <property name="energy" value="6"/>
        <property name="fighting" value="5"/>
        <property name="strength" value="6"/>
    </bean>
</beans>
