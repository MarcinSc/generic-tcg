<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
            http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

    <bean id="setupGameFlow" class="com.gempukku.tcg.generic.flow.MultiActionGameFlow">
        <property name="actions">
            <util:list>
                <bean class="com.gempukku.tcg.generic.player.ForEachPlayerSimultaneouslyAction">
                    <property name="playerAttribute" value="player"/>
                    <property name="gameAction">
                        <bean class="com.gempukku.tcg.generic.action.GameEffectAction">
                            <property name="indexAttribute" value="effectIndex.*{player}"/>
                            <property name="effects">
                                <util:list>
                                    <bean id="selectStartingCharacters"
                                          class="com.gempukku.tcg.generic.stack.SelectArbitraryCardsFromStackEffect">
                                        <property name="attributeName" value="selectedCharacters.*{player}"/>
                                        <property name="min" value="3"/>
                                        <property name="max" value="3"/>
                                        <property name="player" value="*{player}"/>
                                        <property name="stack" value="setupCharactersZone"/>
                                        <property name="message" value="Choose starting characters"/>
                                    </bean>
                                    <bean id="setAsFrontLine"
                                          class="com.gempukku.tcg.generic.effect.SetObjectsAttributeEffect">
                                        <property name="ids" value="*{selectedCharacters.*{player}}"/>
                                        <property name="attributeName" value="position"/>
                                        <property name="attributeValue" value="FrontLine"/>
                                    </bean>
                                    <bean id="putStartingCharactersInFrontLine"
                                          class="com.gempukku.tcg.generic.stack.MoveObjectsBetweenStacksEffect">
                                        <property name="player" value="*{player}"/>
                                        <property name="stackFrom" value="setupCharactersZone"/>
                                        <property name="stackTo" value="inPlayZone"/>
                                        <property name="ids" value="*{selectedCharacters.*{player}}"/>
                                    </bean>
                                    <bean id="listRemainingStartingCharacters"
                                          class="com.gempukku.tcg.generic.stack.ListObjectsInStackEffect">
                                        <property name="attributeName" value="remainingCharacters.*{player}"/>
                                        <property name="player" value="*{player}"/>
                                        <property name="stack" value="setupCharactersZone"/>
                                    </bean>
                                    <bean id="setAsReserve"
                                          class="com.gempukku.tcg.generic.effect.SetObjectsAttributeEffect">
                                        <property name="ids" value="*{remainingCharacters.*{player}}"/>
                                        <property name="attributeName" value="position"/>
                                        <property name="attributeValue" value="Reserve"/>
                                    </bean>
                                    <bean id="putRemainingCharactersInReserve"
                                          class="com.gempukku.tcg.generic.stack.MoveObjectsBetweenStacksEffect">
                                        <property name="player" value="*{player}"/>
                                        <property name="stackFrom" value="setupCharactersZone"/>
                                        <property name="stackTo" value="inPlayZone"/>
                                        <property name="ids" value="*{remainingCharacters.*{player}}"/>
                                    </bean>
                                    <bean id="setupDeck"
                                          class="com.gempukku.tcg.generic.stack.MoveObjectsBetweenStacksEffect">
                                        <property name="player" value="*{player}"/>
                                        <property name="stackFrom" value="setupDeckZone"/>
                                        <property name="stackTo" value="deckZone"/>
                                    </bean>
                                    <bean id="shuffleDeck" class="com.gempukku.tcg.generic.stack.ShuffleStackEffect">
                                        <property name="player" value="*{player}"/>
                                        <property name="stack" value="deckZone"/>
                                    </bean>
                                </util:list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <bean id="endSetupPhase" class="com.gempukku.tcg.generic.action.GameEffectAction">
                    <property name="effects">
                        <util:list>
                            <bean class="com.gempukku.tcg.generic.phase.SetPhaseEffect">
                                <property name="value" value="DrawAndDiscard"/>
                            </bean>
                        </util:list>
                    </property>
                </bean>
            </util:list>
        </property>
    </bean>

    <bean id="drawAndDiscardFlow" class="com.gempukku.tcg.generic.flow.MultiActionGameFlow">
        <property name="actions">
            <util:list>
                <bean class="com.gempukku.tcg.generic.player.ForEachPlayerSimultaneouslyAction">
                    <property name="playerAttribute" value="player"/>
                    <property name="gameAction">
                        <bean class="com.gempukku.tcg.generic.action.GameEffectAction">
                            <property name="indexAttribute" value="effectIndex.*{player}"/>
                            <property name="effects">
                                <util:list>
                                    <bean id="drawEightCards"
                                          class="com.gempukku.tcg.generic.stack.MoveTopObjectsBetweenStacksEffect">
                                        <property name="stackFrom" value="deckZone"/>
                                        <property name="stackTo" value="handZone"/>
                                        <property name="count" value="8"/>
                                        <property name="player" value="*{player}"/>
                                    </bean>
                                    <bean id="discardDuplicatePowerCards"
                                          class="com.gempukku.tcg.generic.effect.RepeatWhileEffect">
                                        <property name="indexAttribute" value="repeatIndex.*{player}"/>
                                        <property name="condition">
                                            <bean class="com.gempukku.tcg.overpower.condition.HasDuplicatePowerCardsCondition">
                                                <property name="player" value="*{player}"/>
                                            </bean>
                                        </property>
                                        <property name="effects">
                                            <util:list>
                                                <bean class="com.gempukku.tcg.generic.effect.context.RemoveContextAttributeEffect">
                                                    <property name="attributeName" value="selectedCards.*{player}"/>
                                                </bean>
                                                <bean class="com.gempukku.tcg.overpower.effect.FindDuplicatePowerCardsEffect">
                                                    <property name="player" value="*{player}"/>
                                                    <property name="attributeName"
                                                              value="duplicateCharacters.*{player}"/>
                                                </bean>
                                                <bean class="com.gempukku.tcg.generic.stack.SelectDigitalObjectsByIdEffect">
                                                    <property name="ids" value="*{duplicateCharacters.*{player}}"/>
                                                    <property name="player" value="*{player}"/>
                                                    <property name="message"
                                                              value="Choose duplicate power card to discard"/>
                                                    <property name="attributeName" value="selectedCards.*{player}"/>
                                                    <property name="min" value="1"/>
                                                    <property name="max" value="1"/>
                                                </bean>
                                                <bean class="com.gempukku.tcg.generic.stack.MoveObjectsBetweenStacksEffect">
                                                    <property name="stackFrom" value="handZone"/>
                                                    <property name="stackTo" value="powerPackZone"/>
                                                    <property name="player" value="*{player}"/>
                                                    <property name="ids" value="*{selectedCards.*{player}}"/>
                                                </bean>
                                            </util:list>
                                        </property>
                                    </bean>
                                    <bean id="listAllCardsInHand"
                                          class="com.gempukku.tcg.generic.stack.ListObjectsInStackEffect">
                                        <property name="attributeName" value="cardsInHand.*{player}"/>
                                        <property name="player" value="*{player}"/>
                                        <property name="stack" value="handZone"/>
                                    </bean>
                                    <bean class="com.gempukku.tcg.generic.stack.SelectDigitalObjectsByIdEffect">
                                        <property name="ids" value="*{cardsInHand.*{player}}"/>
                                        <property name="player" value="*{player}"/>
                                        <property name="message" value="Choose unusable cards to discard"/>
                                        <property name="attributeName" value="unusableCards.*{player}"/>
                                        <property name="min" value="0"/>
                                        <property name="max" value="1000"/>
                                    </bean>
                                    <bean class="com.gempukku.tcg.generic.effect.ConditionalEffect">
                                        <property name="condition">
                                            <bean class="com.gempukku.tcg.generic.condition.HasAttributeSetCondition">
                                                <property name="name" value="unusableCards.*{player}"/>
                                            </bean>
                                        </property>
                                        <property name="effect">
                                            <bean class="com.gempukku.tcg.generic.stack.MoveObjectsBetweenStacksEffect">
                                                <property name="ids" value="*{unusableCards.*{player}}"/>
                                                <property name="stackFrom" value="handZone"/>
                                                <property name="stackTo" value="deadPileZone"/>
                                                <property name="player" value="*{player}"/>
                                            </bean>
                                        </property>
                                    </bean>
                                </util:list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <bean id="endDrawAndDiscardPhase" class="com.gempukku.tcg.generic.action.GameEffectAction">
                    <property name="effects">
                        <util:list>
                            <bean class="com.gempukku.tcg.generic.phase.SetPhaseEffect">
                                <property name="value" value="Placing"/>
                            </bean>
                        </util:list>
                    </property>
                </bean>
            </util:list>
        </property>
    </bean>

    <bean id="placingFlow" class="com.gempukku.tcg.generic.flow.MultiActionGameFlow">
        <property name="actions">
            <util:list>
                <bean class="com.gempukku.tcg.generic.action.GameEffectAction">
                    <property name="effects">
                        <util:list>
                            <bean id="setPassCounter" class="com.gempukku.tcg.generic.effect.context.SetContextAttributeEffect">
                                <property name="name" value="passCount"/>
                                <property name="value" value="0"/>
                            </bean>
                            <bean id="setFirstPlayer" class="com.gempukku.tcg.generic.order.GetFirstPlayerEffect">
                                <property name="attribute" value="player"/>
                            </bean>
                            <bean id="playersPlaceCards" class="com.gempukku.tcg.generic.effect.RepeatWhileEffect">
                                <property name="condition">
                                    <bean class="com.gempukku.tcg.generic.condition.AttributeLessThanCondition">
                                        <property name="name" value="passCount"/>
                                        <property name="value" value="2"/>
                                    </bean>
                                </property>
                                <property name="effects">
                                    <util:list>
                                        <bean class="com.gempukku.tcg.generic.effect.ConditionalEffect">
                                            <property name="condition">
                                                <bean class="com.gempukku.tcg.generic.condition.NotCondition">
                                                    <property name="condition">
                                                        <bean class="com.gempukku.tcg.generic.condition.HasAttributeSetCondition">
                                                            <property name="name" value="passed.*{player}"/>
                                                        </bean>
                                                    </property>
                                                </bean>
                                            </property>
                                            <property name="effect">
                                                <bean class="com.gempukku.tcg.generic.effect.SequenceEffect">
                                                    <property name="effects">
                                                        <util:list>
                                                            <bean id="findPlaceableCards"
                                                                  class="com.gempukku.tcg.generic.stack.ListObjectsInStackEffect">
                                                                <property name="attributeName" value="placeableCards"/>
                                                                <property name="player" value="*{player}"/>
                                                                <property name="stack" value="handZone"/>
                                                                <property name="digitalObjectFilter">
                                                                    <bean class="com.gempukku.tcg.overpower.filter.PlaceableCardsFilter"/>
                                                                </property>
                                                            </bean>
                                                            <bean class="com.gempukku.tcg.generic.effect.context.RemoveContextAttributeEffect">
                                                                <property name="attributeName" value="cardToPlace"/>
                                                            </bean>
                                                            <bean class="com.gempukku.tcg.generic.stack.SelectDigitalObjectsByIdEffect">
                                                                <property name="ids" value="*{placeableCards}"/>
                                                                <property name="player" value="*{player}"/>
                                                                <property name="message"
                                                                          value="Choose card to place"/>
                                                                <property name="attributeName" value="cardToPlace"/>
                                                                <property name="min" value="0"/>
                                                                <property name="max" value="1"/>
                                                            </bean>
                                                            <ref bean="placeSelectedCard"/>
                                                        </util:list>
                                                    </property>
                                                </bean>
                                            </property>
                                        </bean>
                                        <bean class="com.gempukku.tcg.generic.order.GetNextPlayerEffect">
                                            <property name="previous" value="*{player}"/>
                                            <property name="attribute" value="player"/>
                                        </bean>
                                    </util:list>
                                </property>
                            </bean>
                        </util:list>
                    </property>
                </bean>
                <bean id="endPlacingPhase" class="com.gempukku.tcg.generic.action.GameEffectAction">
                    <property name="effects">
                        <util:list>
                            <bean class="com.gempukku.tcg.generic.phase.SetPhaseEffect">
                                <property name="value" value="Venture"/>
                            </bean>
                        </util:list>
                    </property>
                </bean>
            </util:list>
        </property>
    </bean>

    <bean id="placeSelectedCard" class="com.gempukku.tcg.generic.effect.ConditionalEffect">
        <property name="condition">
            <bean class="com.gempukku.tcg.generic.condition.HasAttributeSetCondition">
                <property name="name" value="cardToPlace"/>
            </bean>
        </property>
        <property name="effect">
            <bean class="com.gempukku.tcg.generic.effect.SequenceEffect">
                <property name="indexAttribute"
                          value="placingCardIndex"/>
                <property name="effects">
                    <util:list>
                        <bean class="com.gempukku.tcg.generic.stack.ListObjectsInStackEffect">
                            <property name="stack" value="inPlayZone"/>
                            <property name="player" value="*{player}"/>
                            <property name="attributeName" value="placeableOnCards"/>
                            <property name="digitalObjectFilter">
                                <bean class="com.gempukku.tcg.overpower.filter.PlaceableOnCardsFilter">
                                    <property name="id" value="*{cardToPlace}"/>
                                </bean>
                            </property>
                        </bean>
                        <bean class="com.gempukku.tcg.generic.effect.context.RemoveContextAttributeEffect">
                            <property name="attributeName" value="cardToPlaceOn"/>
                        </bean>
                        <bean class="com.gempukku.tcg.generic.stack.SelectDigitalObjectsByIdEffect">
                            <property name="ids" value="*{placeableOnCards}"/>
                            <property name="message" value="Choose card to place on"/>
                            <property name="min" value="1"/>
                            <property name="max" value="1"/>
                            <property name="player" value="*{player}"/>
                            <property name="attributeName" value="cardToPlaceOn"/>
                        </bean>
                        <bean class="com.gempukku.tcg.generic.effect.SetObjectsAttributeEffect">
                            <property name="ids" value="*{cardToPlace}"/>
                            <property name="attributeName" value="placedOn"/>
                            <property name="attributeValue" value="*{cardToPlaceOn}"/>
                        </bean>
                        <bean class="com.gempukku.tcg.generic.stack.MoveObjectsBetweenStacksEffect">
                            <property name="ids" value="*{cardToPlace}"/>
                            <property name="stackFrom" value="handZone"/>
                            <property name="stackTo" value="inPlayZone"/>
                            <property name="player" value="*{player}"/>
                        </bean>
                    </util:list>
                </property>
            </bean>
        </property>
        <property name="elseEffect">
            <bean class="com.gempukku.tcg.generic.effect.SequenceEffect">
                <property name="indexAttribute" value="elseIndex"/>
                <property name="effects">
                    <util:list>
                        <bean class="com.gempukku.tcg.generic.effect.context.IncrementContextAttributeEffect">
                            <property name="attributeName" value="passCount"/>
                        </bean>
                        <bean class="com.gempukku.tcg.generic.effect.context.SetContextAttributeEffect">
                            <property name="name" value="passed.*{player}"/>
                            <property name="value" value="1"/>
                        </bean>
                    </util:list>
                </property>
            </bean>
        </property>
    </bean>

    <bean id="ventureFlow" class="com.gempukku.tcg.generic.flow.MultiActionGameFlow">
        <property name="actions">
            <util:list>
                <bean class="com.gempukku.tcg.generic.action.GameEffectAction">
                    <property name="effects">
                        <util:list>
                            <bean class="com.gempukku.tcg.generic.effect.context.SetContextAttributeEffect">
                                <property name="name" value="venturedPlayerCount"/>
                                <property name="value" value="0"/>
                            </bean>
                            <bean class="com.gempukku.tcg.generic.order.GetFirstPlayerEffect">
                                <property name="attribute" value="player"/>
                            </bean>
                            <bean class="com.gempukku.tcg.generic.effect.RepeatWhileEffect">
                                <property name="condition">
                                    <bean class="com.gempukku.tcg.generic.condition.AttributeLessThanCondition">
                                        <property name="name" value="venturedPlayerCount"/>
                                        <property name="value" value="2"/>
                                    </bean>
                                </property>
                                <property name="effects">
                                    <util:list>
                                        <bean class="com.gempukku.tcg.generic.effect.context.RemoveContextAttributeEffect">
                                            <property name="attributeName" value="venturedCompletedMissionsCount,venturedReserveMissionsCount,venturedMissionsCount,venturing"/>
                                        </bean>
                                        <bean class="com.gempukku.tcg.generic.effect.context.SetContextAttributeEffect">
                                            <property name="name" value="venturedMissionsCount"/>
                                            <property name="value" value="0"/>
                                        </bean>
                                        <bean class="com.gempukku.tcg.generic.effect.RepeatWhileEffect">
                                            <property name="indexAttribute" value="venturing"/>
                                            <property name="condition">
                                                <bean class="com.gempukku.tcg.generic.condition.AttributeLessThanCondition">
                                                    <property name="name" value="venturedMissionsCount"/>
                                                    <property name="value" value="1"/>
                                                </bean>
                                            </property>
                                            <property name="effects">
                                                <util:list>
                                                    <bean class="com.gempukku.tcg.generic.effect.context.RemoveContextAttributeEffect">
                                                        <property name="attributeName" value="venturedCompletedMissionsCount,venturedReserveMissionsCount"/>
                                                    </bean>
                                                    <bean class="com.gempukku.tcg.generic.stack.GetObjectsInStackCountEffect">
                                                        <property name="player" value="*{player}"/>
                                                        <property name="stack" value="completedMissionsZone"/>
                                                        <property name="attributeName" value="completedMissionsCount"/>
                                                    </bean>
                                                    <bean class="com.gempukku.tcg.generic.effect.decision.ChooseNumberEffect">
                                                        <property name="player" value="*{player}"/>
                                                        <property name="message" value="Choose number of missions to venture from Completed Missions"/>
                                                        <property name="min" value="0"/>
                                                        <property name="max" value="*{completedMissionsCount}"/>
                                                        <property name="attributeName" value="venturedCompletedMissionsCount"/>
                                                    </bean>
                                                    <bean class="com.gempukku.tcg.generic.stack.GetObjectsInStackCountEffect">
                                                        <property name="player" value="*{player}"/>
                                                        <property name="stack" value="reserveMissionsZone"/>
                                                        <property name="attributeName" value="reserveMissionsCount"/>
                                                    </bean>
                                                    <bean class="com.gempukku.tcg.generic.effect.decision.ChooseNumberEffect">
                                                        <property name="player" value="*{player}"/>
                                                        <property name="message" value="Choose number of missions to venture from Reserve Missions"/>
                                                        <property name="min" value="0"/>
                                                        <property name="max" value="*{reserveMissionsCount}"/>
                                                        <property name="attributeName" value="venturedReserveMissionsCount"/>
                                                    </bean>
                                                    <bean class="com.gempukku.tcg.generic.effect.context.SetContextAttributeEffect">
                                                        <property name="name" value="venturedMissionsCount"/>
                                                        <property name="value">
                                                            <bean class="com.gempukku.tcg.generic.evaluator.AddIntEvaluator">
                                                                <property name="numbers">
                                                                    <util:list>
                                                                        <value>*{venturedCompletedMissionsCount}</value>
                                                                        <value>*{venturedReserveMissionsCount}</value>
                                                                    </util:list>
                                                                </property>
                                                            </bean>
                                                        </property>
                                                    </bean>
                                                </util:list>
                                            </property>
                                        </bean>
                                        <bean class="com.gempukku.tcg.generic.effect.flag.SetFlagEffect">
                                            <property name="flagName" value="ventureCompleted.*{player}"/>
                                            <property name="flagValue" value="*{venturedCompletedMissionsCount}"/>
                                        </bean>
                                        <bean class="com.gempukku.tcg.generic.effect.flag.SetFlagEffect">
                                            <property name="flagName" value="ventureReserve.*{player}"/>
                                            <property name="flagValue" value="*{venturedReserveMissionsCount}"/>
                                        </bean>
                                        <bean class="com.gempukku.tcg.generic.effect.flag.SetFlagEffect">
                                            <property name="flagName" value="ventureTotal.*{player}"/>
                                            <property name="flagValue" value="*{venturedMissionsCount}"/>
                                        </bean>
                                        <bean class="com.gempukku.tcg.generic.effect.ConditionalEffect">
                                            <property name="condition">
                                                <bean class="com.gempukku.tcg.generic.condition.AttributeMoreThanCondition">
                                                    <property name="name" value="venturedMissionsCount"/>
                                                    <property name="value" value="2"/>
                                                </bean>
                                            </property>
                                            <property name="effect">
                                                <bean class="com.gempukku.tcg.generic.player.ForEachOpponentEffect">
                                                    <property name="player" value="*{player}"/>
                                                    <property name="attributeName" value="opponent"/>
                                                    <property name="gameEffect">
                                                        <bean class="com.gempukku.tcg.generic.effect.SequenceEffect">
                                                            <property name="effects">
                                                                <util:list>
                                                                    <bean class="com.gempukku.tcg.generic.effect.context.SetContextAttributeEffect">
                                                                        <property name="name" value="cardsToDraw"/>
                                                                        <property name="value">
                                                                            <bean class="com.gempukku.tcg.generic.evaluator.SubtractIntEvaluator">
                                                                                <property name="base" value="*{venturedMissionsCount}"/>
                                                                                <property name="numbers" value="2"/>
                                                                            </bean>
                                                                        </property>
                                                                    </bean>
                                                                    <bean class="com.gempukku.tcg.generic.effect.context.RemoveContextAttributeEffect">
                                                                        <property name="attributeName" value="wantsToDraw"/>
                                                                    </bean>
                                                                    <bean class="com.gempukku.tcg.generic.effect.decision.ChoiceEffect">
                                                                        <property name="player" value="*{opponent}"/>
                                                                        <property name="message" value="Would you like to draw *{cardsToDraw} cards?"/>
                                                                        <property name="attributeName" value="wantsToDraw"/>
                                                                    </bean>
                                                                    <bean class="com.gempukku.tcg.generic.effect.ConditionalEffect">
                                                                        <property name="condition">
                                                                            <bean class="com.gempukku.tcg.generic.condition.AttributeEqualCondition">
                                                                                <property name="name" value="wantsToDraw"/>
                                                                                <property name="value" value="yes"/>
                                                                            </bean>
                                                                        </property>
                                                                        <property name="effect">
                                                                            <bean class="com.gempukku.tcg.generic.effect.SequenceEffect">
                                                                                <property name="indexAttribute" value="drawAndDiscardIndex"/>
                                                                                <property name="effects">
                                                                                    <util:list>
                                                                                        <bean class="com.gempukku.tcg.generic.stack.MoveTopObjectsBetweenStacksEffect">
                                                                                            <property name="stackFrom" value="deckZone"/>
                                                                                            <property name="stackTo" value="handZone"/>
                                                                                            <property name="count" value="*{cardsToDraw}"/>
                                                                                            <property name="player" value="*{opponent}"/>
                                                                                        </bean>
                                                                                        <bean
                                                                                              class="com.gempukku.tcg.generic.effect.RepeatWhileEffect">
                                                                                            <property name="indexAttribute" value="repeatIndex.*{opponent}"/>
                                                                                            <property name="condition">
                                                                                                <bean class="com.gempukku.tcg.overpower.condition.HasDuplicatePowerCardsCondition">
                                                                                                    <property name="player" value="*{opponent}"/>
                                                                                                </bean>
                                                                                            </property>
                                                                                            <property name="effects">
                                                                                                <util:list>
                                                                                                    <bean class="com.gempukku.tcg.generic.effect.context.RemoveContextAttributeEffect">
                                                                                                        <property name="attributeName" value="selectedCards.*{opponent}"/>
                                                                                                    </bean>
                                                                                                    <bean class="com.gempukku.tcg.overpower.effect.FindDuplicatePowerCardsEffect">
                                                                                                        <property name="player" value="*{opponent}"/>
                                                                                                        <property name="attributeName"
                                                                                                                  value="duplicateCharacters.*{opponent}"/>
                                                                                                    </bean>
                                                                                                    <bean class="com.gempukku.tcg.generic.stack.SelectDigitalObjectsByIdEffect">
                                                                                                        <property name="ids" value="*{duplicateCharacters.*{opponent}}"/>
                                                                                                        <property name="player" value="*{opponent}"/>
                                                                                                        <property name="message"
                                                                                                                  value="Choose duplicate power card to discard"/>
                                                                                                        <property name="attributeName" value="selectedCards.*{opponent}"/>
                                                                                                        <property name="min" value="1"/>
                                                                                                        <property name="max" value="1"/>
                                                                                                    </bean>
                                                                                                    <bean class="com.gempukku.tcg.generic.stack.MoveObjectsBetweenStacksEffect">
                                                                                                        <property name="stackFrom" value="handZone"/>
                                                                                                        <property name="stackTo" value="powerPackZone"/>
                                                                                                        <property name="player" value="*{opponent}"/>
                                                                                                        <property name="ids" value="*{selectedCards.*{opponent}}"/>
                                                                                                    </bean>
                                                                                                </util:list>
                                                                                            </property>
                                                                                        </bean>
                                                                                        <bean
                                                                                              class="com.gempukku.tcg.generic.stack.ListObjectsInStackEffect">
                                                                                            <property name="attributeName" value="cardsInHand.*{opponent}"/>
                                                                                            <property name="player" value="*{opponent}"/>
